public class Solution {
    public double MyPow(double x, int n) {
        if (n == 0) return 1;
        if (x == 1) return 1;
        double res = 0;
        res = x;
        for (int i = 1; i < Math.Abs(n); i++)
        {
            res = res * x;
        }
        if (n > 0) return res;
        else return 1/res;
        return 0;
    }
}