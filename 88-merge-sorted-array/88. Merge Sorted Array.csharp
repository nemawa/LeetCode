public class Solution {
    public void Merge(int[] nums1, int m, int[] nums2, int n) {
        

        int last = m + n - 1;
        int i = 0;
        while (i < n)
        {
            nums1[last] = nums2[i];
            last--;
            i++;
        }
        Array.Sort(nums1);
        
    }
}