public class Solution {
    public string AddBinary(string a, string b) {
        //if (a == "0") return b;
        //if (b == "0") return a;
        int number_one = Convert.ToInt32(a, 2);
        int number_two = Convert.ToInt32(b, 2);

        return Convert.ToString(number_one + number_two, 2);

    }
}